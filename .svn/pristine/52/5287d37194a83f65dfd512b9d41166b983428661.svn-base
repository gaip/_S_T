<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="Prototype.App">
	<Application.Resources>
		<!-- Resources scoped at the Application level should be defined here. -->
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Microsoft.Expression.Prototyping.SketchControls;component/ScrollViewerStyles.xaml"/>
				<ResourceDictionary Source="/Prototype.Screens;component/SketchStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<DataTemplate x:Key="DataGridTemplate">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="100"/>
						<ColumnDefinition Width="200"/>
						<ColumnDefinition Width="100"/>
						<ColumnDefinition Width="100"/>
					</Grid.ColumnDefinitions>
					<TextBlock Grid.Column="0" Text="{Binding Address}"/>
					<TextBlock Grid.Column="1" Text="{Binding Name}"/>
					<TextBlock Grid.Column="2" Text="{Binding Value}"/>
					<TextBlock Grid.Column="3" Text="{Binding Range}"/>
				</Grid>
			</DataTemplate>
		</ResourceDictionary>
	</Application.Resources>
</Application>