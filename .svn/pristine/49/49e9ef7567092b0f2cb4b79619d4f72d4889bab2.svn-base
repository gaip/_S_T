<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:pi="http://schemas.microsoft.com/prototyping/2010/interactivity" xmlns:pc="http://schemas.microsoft.com/prototyping/2010/controls" x:Name="Messages_Name"
	x:Class="Sculi2Screens.Messages"
	Width="640" mc:Ignorable="d" Height="480">
	<UserControl.Resources>
		<Storyboard x:Key="OnClick1"/>
		<Storyboard x:Key="stbPlus">
			<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="AdditionalInfos">
				<DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="{x:Static Visibility.Visible}"/>
			</ObjectAnimationUsingKeyFrames>
			<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(ContentControl.Content)" Storyboard.TargetName="btnPlus">
				<DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="-"/>
			</ObjectAnimationUsingKeyFrames>
		</Storyboard>
	</UserControl.Resources>
	<UserControl.Triggers>
		<EventTrigger RoutedEvent="ButtonBase.Click" SourceName="btnPlus">
			<BeginStoryboard x:Name="stbPlus_BeginStoryboard" Storyboard="{StaticResource stbPlus}"/>
		</EventTrigger>
	</UserControl.Triggers>

	<Grid x:Name="LayoutRoot" Background="White">
		<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TitleLeft-Sketch}" VerticalAlignment="Top" Margin="50,0,0,0"><Run Language="fr-ch" Text="Motor D946 - Messages"/></TextBlock>
		<Button Content="&lt;-" HorizontalAlignment="Left" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Top" Width="50" Height="50" FontSize="48">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pi:NavigateToScreenAction TargetScreen="Sculi2Screens.Screen_1_1"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<Grid x:Name="Passive_errors" Margin="8,0,16,-9" Height="232" VerticalAlignment="Bottom">
			<ScrollViewer Margin="8,42,14.785,37" Style="{DynamicResource ScrollViewer-Sketch}"/>
			<TextBlock Margin="8,42,14.785,0" Style="{DynamicResource TextBlock-Sketch}" Background="#FFA79D9D" Height="26.243" VerticalAlignment="Top"/>
			<TextBlock Margin="22.87,82,41.629,0" Style="{DynamicResource TextBlock-Sketch}" FontSize="14.667" Foreground="#FF606060" Background="White" Height="22" VerticalAlignment="Top"><Run Language="fr-ch" Text=" 100             high     Oil Pressure             Oil pressur low                            01.01.2011 11:33                   3"/></TextBlock>
			<Button x:Name="btnPlus1" Content="+" HorizontalAlignment="Left" Margin="0,82,0,0" Style="{DynamicResource Button-Sketch}" Width="25" Height="23.393" VerticalAlignment="Top"/>
			<pc:SketchRectangleUC HorizontalAlignment="Right" Margin="0,42,116.943,37" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Right" Margin="0,42,284,37" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="199.46,42,0,37" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="122.46,48.486,0,37" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="54,42,0,37" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<TextBlock HorizontalAlignment="Right" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="0,48.486,28.629,0" RenderTransformOrigin="0.5,0.5"><Run Language="fr-ch" Text="Occurrences"/></TextBlock>
			<TextBlock Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="0,48.486,180.846,0" HorizontalAlignment="Right"><Run Language="fr-ch" Text="Time occured"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="232.46,48.486,0,0"><Run Language="fr-ch" Text="Description"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="127.46,48.486,0,0" RenderTransformOrigin="2.87,0.456"><Run Language="fr-ch" Text="Name"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="65,48.486,0,0"><Run Language="fr-ch" Text="Priority"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="22.87,48.486,0,0"><Run Language="fr-ch" Text="Code"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Height="28" Margin="8,10,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Width="116.46"><Run Language="fr-ch" Text="Passive errors"/></TextBlock>
			<Grid x:Name="AdditionalInfos1" Margin="54,98,28.629,8" Visibility="Collapsed" Background="White">
				<pc:SketchRectangleUC Style="{DynamicResource Rectangle-Sketch}" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" Margin="0,0,26.371,2.947"/>
				<TextBlock Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="8,8,55.371,0" Height="123.053" d:LayoutOverrides="HorizontalAlignment"><Run Language="fr-ch" Text="Additional infos:" TextDecorations="Underline"/><LineBreak/><Run Language="fr-ch" Text="RPM:                        1600                   First occurence:       01.01.2011 11:32                "/><LineBreak/><Run Language="fr-ch" Text="Injection quantity:     44                     Last occurence:        01.01.2011 11:33                "/><LineBreak/><Run Language="fr-ch" Text="Colant temperature:  12°"/><LineBreak/><Run Language="fr-ch" Text="Engine Load:              50 %"/><LineBreak/><Run Language="fr-ch" Text="Engine Phase:            Starting Up"/></TextBlock>
			</Grid>
		</Grid>
		<Grid x:Name="Active_errors" Margin="8,62,16,186">
			<ScrollViewer Margin="8,29,14.785,30" Style="{DynamicResource ScrollViewer-Sketch}"/>
			<TextBlock Margin="8,29,14.785,0" Style="{DynamicResource TextBlock-Sketch}" Background="#FFA79D9D" Height="26.243" VerticalAlignment="Top"/>
			<TextBlock Margin="22.87,69,28.629,0" Style="{DynamicResource TextBlock-Sketch}" FontSize="14.667" Foreground="#FF606060" Background="#FFEF6A6A" Height="22" VerticalAlignment="Top"><Run Language="fr-ch" Text=" 100             high     Oil Pressure             Oil pressur low                            01.01.2011 11:33                   3"/></TextBlock>
			<Button x:Name="btnPlus" Content="+" HorizontalAlignment="Left" Margin="0,69,0,0" Style="{DynamicResource Button-Sketch}" Width="25" Height="23.393" VerticalAlignment="Top"/>
			<pc:SketchRectangleUC HorizontalAlignment="Right" Margin="0,29,116.943,50" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Right" Margin="0,29,284,50" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="199.46,29,0,50" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="122.46,35.486,0,50" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<pc:SketchRectangleUC HorizontalAlignment="Left" Margin="54,29,0,50" Style="{DynamicResource Rectangle-Sketch}" Width="1"/>
			<TextBlock HorizontalAlignment="Right" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="0,35.486,28.629,0" RenderTransformOrigin="0.5,0.5"><Run Language="fr-ch" Text="Occurrences"/></TextBlock>
			<TextBlock Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="0,35.486,180.846,0" HorizontalAlignment="Right"><Run Language="fr-ch" Text="Time occured"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="232.46,35.486,0,0"><Run Language="fr-ch" Text="Description"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="127.46,35.486,0,0" RenderTransformOrigin="2.87,0.456"><Run Language="fr-ch" Text="Name"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="65,35.486,0,0"><Run Language="fr-ch" Text="Priority"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="22.87,35.486,0,0"><Run Language="fr-ch" Text="Code"/></TextBlock>
			<TextBlock HorizontalAlignment="Left" Height="28" Margin="8,10,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Width="116.46"><Run Language="fr-ch" Text="Active errors"/></TextBlock>
			<Grid x:Name="AdditionalInfos" Margin="22.87,91,59.759,15" Visibility="Collapsed">
				<pc:SketchRectangleUC Style="{DynamicResource Rectangle-Sketch}" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" Margin="0,0,26.371,17.947"/>
				<TextBlock Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Top" Margin="8,8,55.371,0" Height="100.053" d:LayoutOverrides="HorizontalAlignment" FontSize="13.333"><Run Language="fr-ch" Text="Additional infos:" TextDecorations="Underline"/><LineBreak/><Run Language="fr-ch" Text="RPM:                        1600                   First occurence:       01.01.2011 11:32                "/><LineBreak/><Run Language="fr-ch" Text="Injection quantity:     44                     Last occurence:        01.01.2011 11:33                "/><LineBreak/><Run Language="fr-ch" Text="Colant temperature:  12°"/><LineBreak/><Run Language="fr-ch" Text="Engine Load:              50 %"/><LineBreak/><Run Language="fr-ch" Text="Engine Phase:            Starting Up"/></TextBlock>
			</Grid>
		</Grid>
		<TextBox Style="{DynamicResource Note-Sketch}" TextWrapping="Wrap" Text="To Verify:&#xd;&#xa;- 2 columns or 2 rows ?&#xd;&#xa;" HorizontalAlignment="Right" Width="250" Height="85" VerticalAlignment="Bottom"/>
	</Grid>
</UserControl>