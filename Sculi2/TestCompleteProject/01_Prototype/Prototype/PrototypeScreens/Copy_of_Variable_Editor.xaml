<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:pi="http://schemas.microsoft.com/prototyping/2010/interactivity" xmlns:local="clr-namespace:PrototypeScreens" xmlns:pc="http://schemas.microsoft.com/prototyping/2010/controls" x:Name="Variable_Editor_Name"
	mc:Ignorable="d"
	x:Class="PrototypeScreens.Copy_of_Variable_Editor"
	Width="1024" Height="768">
	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="ProjectDataSources.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<Storyboard x:Key="ShowMainShortcut"/>
			<Storyboard x:Key="UnShowMainShortcut"/>
		</ResourceDictionary>
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" Background="White" DataContext="{Binding Source={StaticResource VariableDataSource}}">
		<VisualStateManager.VisualStateGroups>
			<VisualStateGroup x:Name="VariableEditorApplicationBar">
				<VisualStateGroup.Transitions>
					<VisualTransition GeneratedDuration="0:0:0.5">
						<VisualTransition.GeneratedEasingFunction>
							<QuinticEase EasingMode="EaseInOut"/>
						</VisualTransition.GeneratedEasingFunction>
					</VisualTransition>
				</VisualStateGroup.Transitions>
				<VisualState x:Name="_Default"/>
				<VisualState x:Name="_Visible">
					<Storyboard>
						<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="GridBottom">
							<EasingDoubleKeyFrame KeyTime="0" Value="-61"/>
						</DoubleAnimationUsingKeyFrames>
					</Storyboard>
				</VisualState>
			</VisualStateGroup>
		</VisualStateManager.VisualStateGroups>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
						<i:Interaction.Triggers>
					<i:EventTrigger EventName="MouseLeftButtonDown">
						<pi:ActivateStateAction TargetState="_Default"/>
					</i:EventTrigger>
				</i:Interaction.Triggers>

		<Grid>
			<local:GoBackButton Margin="10" Height="50" Width="50" HorizontalAlignment="Left" VerticalAlignment="Top" />
			<TextBlock Margin="75,10,0,0" Style="{DynamicResource TitleLeft-Sketch}" Background="#FF12D20C"><Run Language="fr-ch" Text="Message Analyzer"/></TextBlock>
		</Grid>
		
		<Grid Grid.Row="1" DataContext="{Binding Source={StaticResource SampleDataSource}}">
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition/>
					<ColumnDefinition Width="3*"/>
				</Grid.ColumnDefinitions>
				<Grid.RowDefinitions>
					<RowDefinition Height="50"/>
					<RowDefinition/>
				</Grid.RowDefinitions>
				<Grid Name="GridSearch" Grid.Column="1" Width="510" HorizontalAlignment="Left" Margin="10,0,0,0">
					<Grid.ColumnDefinitions>
						<ColumnDefinition/>
						<ColumnDefinition Width="Auto"/>
					</Grid.ColumnDefinitions>
						<pc:SketchRectangleUC Style="{DynamicResource Rectangle-Sketch}" Grid.ColumnSpan="2" />
						<TextBlock Margin="10,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Style="{DynamicResource TextBlock-Sketch}" Text="Click here to enter text for searching" FontStyle="Italic"/>
						<TextBlock Text="" FontSize="32" VerticalAlignment="Center" FontFamily="/Prototype.Screens;component/Fonts/#Segoe UI Symbol" Grid.Column="1"/>
				</Grid>
				<Grid x:Name="GridDataGrid" Grid.Column="1" Grid.Row="1" Margin="10">
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
							<ColumnDefinition Width="100"/>
							<ColumnDefinition Width="200"/>
							<ColumnDefinition Width="100"/>
							<ColumnDefinition Width="110"/>
					</Grid.ColumnDefinitions>
					<pc:SketchRectangleUC Style="{DynamicResource Rectangle-Sketch}" Background="#FFBEBEBE" Grid.ColumnSpan="4"/>
					<TextBlock Text="Address" Style="{DynamicResource TextBlock-Sketch}"/>
					<TextBlock Grid.Column="1" Text="Name" Style="{DynamicResource TextBlock-Sketch}"/>
					<TextBlock Grid.Column="2" Text="Value" Style="{DynamicResource TextBlock-Sketch}"/>
					<TextBlock Grid.Column="3" Text="Range" Style="{DynamicResource TextBlock-Sketch}"/>

					<Grid Grid.Row="1" Grid.ColumnSpan="4" Width="510">
						<ListBox Style="{DynamicResource ListBox-Sketch}" DataContext="{Binding Source={StaticResource VariableDataSource}}" ItemsSource="{Binding Variables}" ItemTemplate="{DynamicResource DataGridTemplate}" />
					</Grid>
					<TextBox Margin="-28.667,47.119,20.333,-172.182" Grid.Row="1" Style="{DynamicResource Note-Sketch}" TextWrapping="Wrap" Text="Info&#xd;&#xa;-----&#xd;&#xa;Code&#xd;&#xa;Priority&#xd;&#xa;Name&#xd;&#xa;Description&#xd;&#xa;Time (occured)&#xd;&#xa;First / last occurence&#xd;&#xa;Frequenzy rate (nbr of occurences)&#xd;&#xa;&#xd;&#xa;+ Additional Infos&#xd;&#xa;--------------------&#xd;&#xa;RPM&#xd;&#xa;Injection quantity&#xd;&#xa;Coolant temperature&#xd;&#xa;Engine load (%)&#xd;&#xa;Engine Phase (starting up / cranking / idle speed / emergency mode / engine spooling down...)&#xd;&#xa;&#xd;&#xa;Additional infos availabe using a '+' in front of the error line" FontSize="16" Grid.ColumnSpan="3"/>
					<Image Margin="-198,23.121,0,-123.683" Source="Image5.png" Stretch="Fill" HorizontalAlignment="Left" Grid.Row="1" Width="150"/>
				</Grid>
				<Grid Grid.Row="1" Name="GridBottomLeft" Margin="10">
					<StackPanel>
						<Grid/>
						
					</StackPanel>

				</Grid>
				<pc:SketchRectangleUC Height="20.5" Margin="75,63.5,102,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>
				<pc:SketchRectangleUC Height="20.5" Margin="75,114,102,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>
				<pc:SketchRectangleUC Height="20.5" Margin="75,93.5,102,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>
				<pc:SketchRectangleUC Height="20.5" Margin="75,162.834,102,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>
				<pc:SketchRectangleUC Height="20.5" Margin="75,177.5,80,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>
				<pc:SketchRectangleUC Height="20.5" Margin="75,198,80,0" Grid.Row="1" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Background="#641DED20"/>	
			</Grid>
			<Grid x:Name="GridBottom" VerticalAlignment="Bottom" Height="70" Margin="0,0,0,-63" RenderTransformOrigin="0.5,0.5">
				<i:Interaction.Triggers>
					<i:EventTrigger EventName="MouseLeftButtonDown">
						<pi:ActivateStateAction TargetState="_Visible"/>
					</i:EventTrigger>
				</i:Interaction.Triggers>
				<Grid.RenderTransform>
					<TransformGroup>
						<ScaleTransform/>
						<SkewTransform/>
						<RotateTransform/>
						<TranslateTransform/>
					</TransformGroup>
				</Grid.RenderTransform>
				<pc:SketchRectangleUC Style="{DynamicResource Rectangle-Sketch}" Background="#FFBEBEBE"/>
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
					<StackPanel Margin="5,0,5,0">
						<Grid>
							<pc:SketchCircleUC Height="43"  Style="{DynamicResource Circle-Sketch}" Margin="0,3.5" Width="43"/>
							<TextBlock Text="" FontFamily="/Prototype.Screens;component/Fonts/#Segoe UI Symbol" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Center" FontSize="26.667" HorizontalAlignment="Center"/>
						</Grid>
							<TextBlock Style="{DynamicResource TextBlock-Sketch}" Text="Delete" HorizontalAlignment="Center" VerticalAlignment="Top"/>						
					</StackPanel>
					<StackPanel Margin="5,0,5,0">
						<Grid>
							<pc:SketchCircleUC Height="43"  Style="{DynamicResource Circle-Sketch}" Margin="0,3.5" Width="43"/>
							<TextBlock Text="" FontFamily="/Prototype.Screens;component/Fonts/#Segoe UI Symbol" Style="{DynamicResource TextBlock-Sketch}" VerticalAlignment="Center" FontSize="26.667" HorizontalAlignment="Center"/>
						</Grid>
							<TextBlock Style="{DynamicResource TextBlock-Sketch}" Text="Save" HorizontalAlignment="Center" VerticalAlignment="Top"/>						
					</StackPanel>
				</StackPanel>
			</Grid>
		</Grid>
	</Grid>
</UserControl>