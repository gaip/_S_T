%?partdef pageButton001
%?fn pageButton001
%?brief message symbol neutral, yellow, red or invisible
%?bbox 0 0 79 79
/pageButton001 {
 /I_msgValidNr exch def %?brief Anzahl aller aktuell relevanten Messages %?use INP %?type REAL %?dflt    0.00
 /I_msgAcknNr exch def %?brief Anzahl aller aktuell quittierten, aber noch aktiven Messages %?use INP %?type DINT %?dflt 0
 /I_msgDeactNr exch def %?brief Anzahl aller aktuell relevanten wber nicht mehr aktiven Messages %?use INP %?type DINT %?dflt 0
 I_msgValidNr 0 gt { 
  matrix currentmatrix_nlw 
  currentcolor 
  Invisible
  0 0 translate 
  0 rotate 
  1 1 scale 
  symspemenu008 
  setcolor 
  setmatrix_nlw 
  I_msgValidNr I_msgAcknNr gt {
   matrix currentmatrix_nlw 
   currentcolor 
   true setflash 
   Red
   0 0 translate 
   0 rotate 
   1 1 scale 
   symspemenu008 
   false setflash 
   setcolor 
   setmatrix_nlw 
  } { 
   matrix currentmatrix_nlw 
   currentcolor 
   Yellow
   0 0 translate 
   0 rotate 
   1 1 scale 
   symspemenu008 
   setcolor 
   setmatrix_nlw 
  } ifelse 
 } { 
  matrix currentmatrix_nlw 
  currentcolor 
  0 0 translate 
  0 rotate 
  1 1 scale 
  symspemenu008 
  setcolor 
  setmatrix_nlw 
 } ifelse 
} def
%?partend







