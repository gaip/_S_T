		% Symbol durchgestrichen
		LX_Ton { 
			9 setlinewidth_nlw 
			currentcolor 
			Invisible 10 10 moveto 
			70 70 lineto 
			stroke 
			setcolor 
			3 setlinewidth_nlw 
			10 10 moveto 
			70 70 lineto 
			stroke 
			1 setlinewidth_nlw 
