%?partdef gpButtonPage003
%?fn	gpButtonPage003
%?brief	Button to switch to another page; external buzzer acknowledge is used
%?bbox	0 0 79 79
/gpButtonPage003 {
	/isSymbolName exch def	%?brief name of the symbol-part which has to be displayed %?use INP %?type STRI %?dflt (gsGeneral001)
	/idSymErrActCnt exch def	%?brief number of active error symbols %?use INP %?type DINT %?dflt 0
	/idSymWarnActCnt exch def	%?brief number of active warning symbols %?use INP %?type DINT %?dflt 0
	/ixBuzzerAckDemand exch def	%?brief buzzer acknowledge demand %?use INP %?type BOOL %?dflt false
%?manual_beg
	ixBuzzerAckDemand true eq {	
		idSymErrActCnt 0 gt {	
			true setflash 
			currentcolor 
			Red
			isSymbolName cvx exec
			setcolor 
			false setflash 
		}{
			true setflash 
			currentcolor 
			Yellow
			isSymbolName cvx exec
			setcolor 
			false setflash 
		} ifelse 
	}{ 
		idSymErrActCnt 0 gt
		idSymWarnActCnt 0 gt or {	
			true setflash 
			currentcolor 
			isSymbolName cvx exec
			setcolor 
			false setflash 
		}{
			currentcolor 
			isSymbolName cvx exec
			setcolor 
		} ifelse 
	} ifelse 
%?manual_end
} def
%?partend


%?partdef gpButtonControl001
%?fn	gpButtonControl001
%?brief	Button to switch to invers mode (ATTENTION: this part may only to be setted on a project page directly, not in a other part)
%?bbox	0 0 79 79
/gpButtonControl001 {
	
%?manual_beg
	% Hier darf kein currentcolor - setcolor stehen
	0 0 moveto
	currentcolor					% Farbumschaltungen des Touchbutton kapseln
	baTouchButton001
	exch
	setcolor
	{	
		setcolor 					% das currentcolor der obersten Ebene wird aufgehoben
		(get_inverse_mode) getsysparam {	
			(set_inverse_mode) false setsysparam 
		} { 
			(set_inverse_mode) true setsysparam 
		} ifelse 
		currentcolor			 	% das currentcolor der obersten Ebene wird neu gesetzt
		colorinit					% colorinit der llsystem aufrufen, damit die Fraben richtig gesetzt werden
	} if 
	matrix currentmatrix_nlw 
	currentcolor 
	0 0 translate 
	0 rotate 
	1 1 scale 
	gsGeneral131 
	setcolor 
	setmatrix_nlw 
%?manual_end
	
} def
%?partend

%?partdef gpButtonControl002
%?fn	gpButtonControl002
%?brief	Button to increase brightness
%?bbox	0 0 79 79
/gpButtonControl002 {
	/ixChangeBrightness exch def	%?brief change brightness on external order %?use INP %?type BOOL %?dflt false
	/qdBrightness 0 def	%?brief LCD brightness %?use OUT %?type DINT %?dflt 0
	/LD_Change 0 def	%?brief change brightness %?use LOC %?type DINT %?dflt 0
%?manual_beg
	(lcd_get_brightness) getsysparam 
	/qdBrightness exch store 
	qdBrightness 40 lt { /LD_Change 16 store } if
	qdBrightness 40 ge { /LD_Change 26 store } if
%?manual_end
	qdBrightness 90 lt {
		matrix currentmatrix_nlw 
		currentcolor 
		0 0 translate 
		0 rotate 
		1 1 scale 
		baTouchButton001 {	
			%?usercode_beg
			/ixChangeBrightness true store
			%?usercode_end 
		} if 
		setcolor 
		setmatrix_nlw 
	} if
	qdBrightness 90 ge {
		matrix currentmatrix_nlw 
		currentcolor 
		Grey 0 0 translate 
		0 rotate 
		1 1 scale 
		gsGeneral087 
		setcolor 
		setmatrix_nlw 
	} if
	qdBrightness 90 lt {
		matrix currentmatrix_nlw 
		currentcolor 
		0 0 translate 
		0 rotate 
		1 1 scale 
		gsGeneral087 
		setcolor 
		setmatrix_nlw 
	} if
%?manual_beg
	ixChangeBrightness true eq {
		/qdBrightness qdBrightness LD_Change add store 
		(lcd_set_brightness) 
		qdBrightness setsysparam 
	} if
%?manual_end
	qdBrightness
} def
%?partend

