%?partdef gpCamera002
%?fn	gpCamera002
%?brief	show camera in quarter VGA
%?bbox	0 0 319 239
/gpCamera002 {
	/MEM_startTime_00 exch def	%?brief  %?use STATIC %?type REAL
	/idDummy1 exch def	%?brief dummy 1 %?use INP %?type REAL %?dflt    0.00
	/isShowText exch def	%?brief camera discription text shown %?use INP %?type STRI %?dflt  
	/irContrast exch def	%?brief camera contrast %?use INP %?type REAL %?dflt 75.00
	/ixFlip exch def	%?brief camera flipped (vertical) %?use INP %?type BOOL %?dflt false
	/ixMirror exch def	%?brief camera mirrored (horizontal) %?use INP %?type BOOL %?dflt false
	/ixCamActive exch def	%?brief camera is active %?use INP %?type BOOL %?dflt false
	/isCamSymName exch def	%?brief name of the camera symbol %?use INP %?type STRI %?dflt gsGeneral034
	/ixUseInternalTuple exch def	%?brief use internal tuple %?use INP %?type BOOL %?dflt    false
	/idCameraQual exch def	%?brief quality %?use INP %?type DINT %?dflt 0
	/isCameraName exch def	%?brief name of camera %?use INP %?type STRI %?dflt cam0
	/qrBoardTemp 0 def	%?brief camera board temperature %?use OUT %?type REAL %?dflt    0.00

%?manual_beg
	/ldCameraSizeX 320 def	%?brief size of camera image %?use LOC %?type DINT %?dflt 320
	/ldCameraSizeY 240 def	%?brief size of camera image %?use LOC %?type DINT %?dflt 240
	/lsCameraSize ()  def	%?brief size of camera image %?use LOC %?type STRI %?dflt 
	/ldCameraPositionX 80 def	%?brief under left corner of camera image  %?use LOC %?type DINT %?dflt 80
	/ldCameraPositionY 80 def	%?brief under left corner of camera image  %?use LOC %?type DINT %?dflt 80
	/lsCameraPosition () def	%?brief under left corner of camera image  %?use LOC %?type STRI %?dflt
	/LS_Help () def 	%?brief helper string  %?use LOC %?type STRI %?dflt
	/LX_Ton () def 	%?brief temp var  %?use LOC %?type BOOL %?dflt false
	
	% Verzögerungszeit für Symbol durchgestrichen
	ixCamActive 2.0 MEM_startTime_00
	FUB_TON 
	/MEM_startTime_00 exch store
 	/LX_Ton exch store
 	pop 								% ET (elapsed time) Ausgang des FUB_TON vom stack löschen
	
	ixCamActive {
		0 0
		matrix currentmatrix
		transform
		/ldCameraPositionY exch store
		/ldCameraPositionX exch store
		
		0 0 ldCameraSizeX 1 sub ldCameraSizeY 1 sub rectstroke
		matrix currentmatrix
		120 100 translate
		isCamSymName cvx exec
		% Symbol durchgestrichen
		LX_Ton { 
			9 setlinewidth_nlw 
			currentcolor 
			Invisible 10 10 moveto 
			70 70 lineto 
			stroke 
			setcolor 
			3 setlinewidth_nlw 
			10 10 moveto 
			70 70 lineto 
			stroke 
