		( ) LS_Help cvs strcat
		ldCameraSizeY LS_Help cvs strcat
		/lsCameraSize exch store
		
		(camera_select) isCameraName setsysparam 
		(camera_size) lsCameraSize setsysparam 
		(camera_pos) lsCameraPosition setsysparam 
		(camera_quality) idCameraQual setsysparam 
		ixUseInternalTuple true eq {
			(camera_internal) 1 setsysparam 
		} {
			ixMirror { (camera_mirror) 1 setsysparam }{ (camera_mirror) 0 setsysparam } ifelse
			ixFlip { (camera_flip) 1 setsysparam }{ (camera_flip) 0 setsysparam } ifelse
			(camera_contrast) irContrast setsysparam 
		} ifelse
		(camera_start) () setsysparam 
		(camera_show) 1 setsysparam 
		(camera_board_temp) getsysparam
		/qrBoardTemp exch store
	} {
		(camera_select) isCameraName setsysparam
		(camera_show) 0 setsysparam					% Kamera abschalten
	} ifelse
%?manual_end
	qrBoardTemp MEM_startTime_00 
